{
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "svelteBracketNewLine": false,
    "bracketSameLine": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "singleAttributePerLine": true,
    "printWidth": 3000
}
